{
  "name": "atlas-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5400",
    "dev:host": "vite --port 5400 --host",
    "build": "tsc && vite build",
    "build-only": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --format visualstudio --max-warnings 100",
    "lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --format visualstudio --max-warnings 100",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,md}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/fabric": "^5.3.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "fabric": "^5.3.0",
    "i18next": "^23.5.1",
    "i18next-http-backend": "^2.2.2",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.2.2",
    "react-redux": "^8.1.3",
    "react-responsive-modal": "^6.4.2",
    "react-router-dom": "^6.16.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-gzip": "^3.1.0",
    "sass": "^1.69.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
